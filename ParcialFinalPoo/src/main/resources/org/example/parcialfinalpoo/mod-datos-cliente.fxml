<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="480.0" prefWidth="854.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.parcialfinalpoo.controllers.ModDatosClienteController">
   <children>
      <BorderPane prefHeight="480.0" prefWidth="854.0">
         <center>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Label text="Seleccione lo que desea modificar">
                     <font>
                        <Font size="24.0" />
                     </font></Label>
                  <HBox alignment="CENTER" prefHeight="232.0" prefWidth="854.0" spacing="50.0">
                     <children>
                        <Button fx:id="btnDatosClientes" mnemonicParsing="false" onAction="#btnDatosClientesOnAction" text="Datos del Cliente">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Button>
                        <Button fx:id="btnTarjetasClientes" mnemonicParsing="false" onAction="#btnTarjetasClientesOnAction" text="Tarjetas del Cliente">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Button>
                     </children>
                  </HBox>
                  <Button fx:id="btnSalir7" mnemonicParsing="false" onAction="#btnSalir7OnAction" text="Salir">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </center></BorderPane>
   </children>
</AnchorPane>
